{
	"indentation": {
		"tabWidth": 2
	},
	"sameLine": {
		"ifBody": "same",
		"elseBody": "same",
		"comprehensionFor": "keep",
		"forBody": "keep"
	},
	"wrapping": {
		"maxLineLength": 90,
		"arrayMatrixWrap": "matrixWrapNoAlign",
		"arrayWrap": {
			"defaultWrap": "keep"
		},
		"functionSignature": {
			"defaultWrap": "keep"
		},
		"callParameter": {
			"defaultWrap": "keep"
		},
		"methodChain": {
			"rules": []
		},
		"opAddSubChain": {
			"rules": [
				{
					"conditions": [
						{
							"cond": "anyItemLength >= n",
							"value": 160
						}
					],
					"location": "beforeLast",
					"type": "onePerLineAfterFirst"
				}
			]
		}
	},
	"emptyLines": {
		"beforeDocCommentEmptyLines": "ignore",
		"afterFieldsWithDocComments": "ignore"
	}
}
